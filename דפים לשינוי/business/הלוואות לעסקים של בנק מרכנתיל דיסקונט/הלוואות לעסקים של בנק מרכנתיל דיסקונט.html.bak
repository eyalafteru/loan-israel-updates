<script>
(function() {
  'use strict';

  var ns = 'WPC_Loans_M7K3_FAQ';
  if (window[ns]) return;
  window[ns] = true;

  var faqContainer = document.querySelector('.wpc-loans-m7k3-faq-container');
  if (!faqContainer) return;

  faqContainer.addEventListener('click', function(e) {
    var header = e.target.closest('[data-action="toggle"]');
    if (!header) return;

    e.preventDefault();

    var item = header.closest('.wpc-loans-m7k3-faq-item');
    if (!item) return;

    var content = item.querySelector('.wpc-loans-m7k3-faq-content');
    if (!content) return;

    var isActive = header.classList.contains('active');

    var allHeaders = faqContainer.querySelectorAll('.wpc-loans-m7k3-faq-header');
    var allContents = faqContainer.querySelectorAll('.wpc-loans-m7k3-faq-content');

    for (var i = 0; i < allHeaders.length; i++) {
      allHeaders[i].classList.remove('active');
    }
    for (var j = 0; j < allContents.length; j++) {
      allContents[j].classList.remove('active');
    }

    if (!isActive) {
      header.classList.add('active');
      content.classList.add('active');
    }
  });

  // Smooth scroll for CTA buttons
  var ctaButtons = document.querySelectorAll('.wpc-loans-m7k3-cta-btn[href^="#"]');
  for (var k = 0; k < ctaButtons.length; k++) {
    ctaButtons[k].addEventListener('click', function(e) {
      var targetId = this.getAttribute('href').substring(1);
      var targetElement = document.getElementById(targetId);

      if (targetElement) {
        e.preventDefault();
        targetElement.scrollIntoView({
          behavior: 'smooth',
          block: 'start'
        });
      }
    });
  }
})();
</script>

