<!-- FAQ Accordion JavaScript -->
<script>
(function() {
  'use strict';

  const ns = 'WPC_Loans_G7K3_FAQ';
  if (window[ns]) return;
  window[ns] = true;

  // Wait for DOM to be ready
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', initFAQ);
  } else {
    initFAQ();
  }

  function initFAQ() {
    const faqContainer = document.querySelector('.wpc-loans-g7k3-faq-container');
    if (!faqContainer) return;

    // Add click event to FAQ container using event delegation
    faqContainer.addEventListener('click', function(e) {
      const header = e.target.closest('.wpc-loans-g7k3-faq-header');
      if (!header) return;

      const item = header.closest('.wpc-loans-g7k3-faq-item');
      if (!item) return;

      // Toggle active class
      item.classList.toggle('active');
    });
  }
})();
</script>

