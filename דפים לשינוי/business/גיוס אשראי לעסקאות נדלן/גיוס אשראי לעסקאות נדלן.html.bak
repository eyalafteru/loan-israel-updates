<script>
(function() {
  'use strict';
  var ns = 'WPC_Loans_R3E8_FAQ';
  if (window[ns]) return;
  window[ns] = true;

  var container = document.querySelector('.wpc-loans-r3e8-faq-container');
  if (!container) return;

  container.addEventListener('click', function(e) {
    var button = e.target.closest('[data-action="toggle-faq"]');
    if (!button) return;

    e.preventDefault();
    var faqItem = button.closest('.wpc-loans-r3e8-faq-item');
    if (!faqItem) return;

    var isActive = faqItem.classList.contains('active');

    var allItems = container.querySelectorAll('.wpc-loans-r3e8-faq-item');
    allItems.forEach(function(item) {
      item.classList.remove('active');
    });

    if (!isActive) {
      faqItem.classList.add('active');
    }
  });
})();

// Smooth scroll to form for CTA buttons
(function() {
  'use strict';
  var ns = 'WPC_Loans_R3E8_ScrollToForm';
  if (window[ns]) return;
  window[ns] = true;

  document.addEventListener('click', function(e) {
    var link = e.target.closest('.scroll-to-form');
    if (!link) return;

    var href = link.getAttribute('href');
    if (!href || !href.startsWith('#')) return;

    e.preventDefault();
    
    var targetId = href.substring(1);
    var targetElement = document.getElementById(targetId);

    if (targetElement) {
      var offset = 100; // Offset for fixed headers
      var elementPosition = targetElement.getBoundingClientRect().top + window.pageYOffset;
      var offsetPosition = elementPosition - offset;

      window.scrollTo({
        top: offsetPosition,
        behavior: 'smooth'
      });
    } else {
      // Fallback: try to scroll to the element using the href
      try {
        var element = document.querySelector(href);
        if (element) {
          element.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }
      } catch(err) {
        console.log('Could not scroll to element:', href);
      }
    }
  });
})();
</script>

</div>