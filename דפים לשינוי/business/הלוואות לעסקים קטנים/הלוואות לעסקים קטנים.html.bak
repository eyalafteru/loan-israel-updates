<!-- ===== JavaScript for FAQ Accordion ===== -->
<script>
(function() {
  'use strict';

  var ns = 'WPC_Loans_K3M7';
  if (window[ns]) return;
  window[ns] = true;

  var faqContainer = document.querySelector('.wpc-loans-k3m7-faq-container');
  if (!faqContainer) return;

  faqContainer.addEventListener('click', function(e) {
    var button = e.target.closest('[data-action="toggle"]');
    if (!button) return;

    var item = button.closest('.wpc-loans-k3m7-faq-item');
    if (!item) return;

    var isActive = item.classList.contains('active');

    var allItems = faqContainer.querySelectorAll('.wpc-loans-k3m7-faq-item');
    allItems.forEach(function(i) {
      i.classList.remove('active');
    });

    if (!isActive) {
      item.classList.add('active');
    }
  });
})();
</script>

