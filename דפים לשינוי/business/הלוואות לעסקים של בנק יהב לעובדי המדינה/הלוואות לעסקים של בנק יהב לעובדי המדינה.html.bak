<script>
(function() {
  'use strict';

  var ns = 'WPC_Loans_Y4H6_FAQ';
  if (window[ns]) return;
  window[ns] = true;

  var faqContainer = document.querySelector('.wpc-loans-y4h6-faq-container');
  if (!faqContainer) return;

  faqContainer.addEventListener('click', function(e) {
    var button = e.target.closest('[data-action="wpc-loans-y4h6-toggle-faq"]');
    if (!button) return;

    var item = button.closest('.wpc-loans-y4h6-faq-item');
    if (!item) return;

    var isActive = item.classList.contains('active');

    var allItems = faqContainer.querySelectorAll('.wpc-loans-y4h6-faq-item');
    allItems.forEach(function(i) {
      i.classList.remove('active');
    });

    if (!isActive) {
      item.classList.add('active');
    }
  });
})();
</script>

