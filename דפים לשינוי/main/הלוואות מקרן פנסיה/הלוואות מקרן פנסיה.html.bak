    // ğŸ§® ×—×™×©×•×‘ ×”×©×•×•××ª ×”×œ×•×•××•×ª
    window.calculateLoans = function() {
        const pensionBalance = parseInt(document.getElementById('pension-balance').value);
        const loanAmount = parseInt(document.getElementById('loan-amount').value);
        const repaymentPeriod = parseInt(document.getElementById('repayment-period').value);
        const age = parseInt(document.getElementById('age').value);
        const monthlyIncome = parseInt(document.getElementById('monthly-income').value);
        
        // ×•×œ×™×“×¦×™×•×ª
        if (loanAmount > pensionBalance * 0.35) {
            alert('×¡×›×•× ×”×”×œ×•×•××” ×¢×•×œ×” ×¢×œ ×”××§×¡×™××•× ×”××•×ª×¨ (35% ××”×™×ª×¨×”)');
            return;
        }
        
        if (age < 21 || age > 70) {
            alert('×”×’×™×œ ×—×™×™×‘ ×œ×”×™×•×ª ×‘×™×Ÿ 21-70 ×©× ×™×');
