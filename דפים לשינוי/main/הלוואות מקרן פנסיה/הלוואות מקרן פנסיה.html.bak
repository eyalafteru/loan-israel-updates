                document.getElementById(`rate-${companyKey}`).textContent = `${company.baseRate}%`;
                document.getElementById(`total-${companyKey}`).textContent = formatCurrency(totalInterest);
                document.getElementById(`approval-${companyKey}`).textContent = `${company.approvalDays} ×™××™×`;
                
