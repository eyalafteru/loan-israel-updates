        document.getElementById('sim-total').textContent = formatCurrency(totalPayment);
        
        // ×‘× ×™×™×ª ×œ×•×— ×ª×©×œ×•××™×
        buildPaymentTable(amount, rate, years * 12);
    }
    
    // ğŸ“… ×‘× ×™×™×ª ×œ×•×— ×ª×©×œ×•××™×
    function buildPaymentTable(principal, rate, months) {
        const table = document.getElementById('payment-table');
        const monthlyPayment = calculatePensionLoanPMT(principal, rate, months);
        const monthlyRate = rate / 100 / 12;
        
        let html = `
        <table style="width: 100%; font-family: Assistant, sans-serif;">
            <thead>
                <tr style="background: var(--primary); color: white;">
                    <th style="padding: 12px; text-align: right;">×ª×©×œ×•×</th>
                    <th style="padding: 12px; text-align: right;">×§×¨×Ÿ</th>
                    <th style="padding: 12px; text-align: right;">×¨×™×‘×™×ª</th>
                    <th style="padding: 12px; text-align: right;">×™×ª×¨×”</th>
                </tr>
            </thead>
            <tbody>
        `;
        
        let remainingBalance = principal;
        
        // ×”×¦×’×ª ×¨×§ 12 ×”×ª×©×œ×•××™× ×”×¨××©×•× ×™× + ×”××—×¨×•×Ÿ
        for (let i = 1; i <= Math.min(12, months); i++) {
            const interestPayment = remainingBalance * monthlyRate;
            const principalPayment = monthlyPayment - interestPayment;
            remainingBalance -= principalPayment;
            
            html += `
            <tr style="border-bottom: 1px solid #eee;">
                <td style="padding: 10px; text-align: right;">${formatCurrency(monthlyPayment)}</td>
                <td style="padding: 10px; text-align: right;">${formatCurrency(principalPayment)}</td>
                <td style="padding: 10px; text-align: right;">${formatCurrency(interestPayment)}</td>
                <td style="padding: 10px; text-align: right;">${formatCurrency(remainingBalance)}</td>
            </tr>
            `;
        }
        
        if (months > 12) {
            html += `
            <tr style="text-align: center; font-style: italic; color: #666;">
                <td colspan="4" style="padding: 15px;">... ${months - 12} ×ª×©×œ×•××™× × ×•×¡×¤×™× ...</td>
            </tr>
            `;
        }
        
        html += '</tbody></table>';
        table.innerHTML = html;
    }
    
