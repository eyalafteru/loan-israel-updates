        futureValueLoss: container.querySelector('#futureValueLoss'),
        percentageImpact: container.querySelector('#percentageImpact'),
        bankLoanCost: container.querySelector('#bankLoanCost'),
        yearsToRetirement: container.querySelector('#yearsToRetirement'),
        loanPercentage: container.querySelector('#loanPercentage'),
