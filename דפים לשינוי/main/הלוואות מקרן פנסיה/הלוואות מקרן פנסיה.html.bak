    };
    
    // ğŸ§® ×—×™×©×•×‘ ×¢×œ×•×ª ×”×–×“×× ×•×ª ×¤× ×¡×™×•× ×™×ª (× ×•×¡×—×” ××”××™×¤×™×•×Ÿ)
    function calculatePensionOpportunityCost(loanAmount, yearsToRetirement, monthlyDeposit, expectedReturn = 5) {
        // ×›××” ×”×›×¡×£ ×”×™×” ×©×•×•×” ×‘×¤×¨×™×©×” ×× ×œ× ×œ×•×§×—×™× ×”×œ×•×•××”
        const futureValueWithoutLoan = loanAmount * Math.pow(1 + expectedReturn/100, yearsToRetirement);
        
        // ×›××” ×¤× ×¡×™×” ×—×•×“×©×™×ª ×–×” ××•××¨ (×—×œ×•×§×” ×¢×œ 20 ×©× ×•×ª ×ª×•×—×œ×ª ×—×™×™×)
        const monthlyPensionLoss = futureValueWithoutLoan / (20 * 12);
        
        // ×—×™×©×•×‘ ×¦×‘×™×¨×” ×¢×ª×™×“×™×ª ×¢× ×•×”×œ×œ× ×”×œ×•×•××”
        const totalFutureWithoutLoan = (monthlyDeposit * 12 * yearsToRetirement * Math.pow(1 + expectedReturn/100, yearsToRetirement/2)) + futureValueWithoutLoan;
        const totalFutureWithLoan = (monthlyDeposit * 12 * yearsToRetirement * Math.pow(1 + expectedReturn/100, yearsToRetirement/2));
        
        // ××—×•×– ××”×¤× ×¡×™×” ×”×¢×ª×™×“×™×ª
        const futurePensionMonthly = totalFutureWithoutLoan / (20 * 12);
        const percentageOfFuturePension = (monthlyPensionLoss / futurePensionMonthly) * 100;
        
        return {
            futureValueLoss: futureValueWithoutLoan,
