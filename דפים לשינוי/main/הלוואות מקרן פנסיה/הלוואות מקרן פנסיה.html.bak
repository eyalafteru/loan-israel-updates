                const maxLoanForCompany = calculateMaxLoan(pensionBalance, companyKey);
                const actualLoanAmount = Math.min(loanAmount, maxLoanForCompany);
                
                const monthlyPayment = calculatePensionLoanPMT(
                    actualLoanAmount, 
                    company.baseRate, 
                    repaymentPeriod * 12
