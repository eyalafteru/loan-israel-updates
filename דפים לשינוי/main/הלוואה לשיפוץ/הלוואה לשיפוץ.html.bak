    <script>
    (function() {
        'use strict';
        
        const wrapper = document.getElementById('shiputz24-wrapper');
        if (!wrapper) return;
        
        // Renovation Calculator Variables
        let selectedSize = 0;
        let selectedType = 1;
        let selectedAddons = [];
        let selectedSizeText = '';
        let selectedTypeText = '';
        
        // Back Navigation Functions
        window.goBackToStep = function(stepNumber) {
            showStep(stepNumber);
            
            // Hide result if going back
            const resultDiv = document.getElementById('calcResult');
            if (resultDiv) {
                resultDiv.style.display = 'none';
            }
        };
        
        window.editCalculation = function() {
            // Go back to first step and reset
            showStep(1);
            
            // Hide result
            const resultDiv = document.getElementById('calcResult');
            if (resultDiv) {
                resultDiv.style.display = 'none';
            }
            
            // Keep the selections but allow re-editing
            // Don't reset the values so user can see their previous choices
        };
        
        window.restartCalculation = function() {
            // Reset all values
            selectedSize = 0;
            selectedType = 1;
            selectedAddons = [];
            selectedSizeText = '';
            selectedTypeText = '';
            
            // Remove all selections
            wrapper.querySelectorAll('.shiputz24-calculator .shiputz24-calc-option.selected').forEach(option => {
                option.classList.remove('selected');
            });
            wrapper.querySelectorAll('.shiputz24-calculator .shiputz24-calc-checkbox.selected').forEach(checkbox => {
                checkbox.classList.remove('selected');
            });
            
            // Reset progress indicators
            wrapper.querySelectorAll('.shiputz24-calculator .shiputz24-calc-progress__dot').forEach((dot, index) => {
                dot.classList.remove('active', 'completed');
                if (index === 0) dot.classList.add('active');
            });
            
            // Go back to first step
            showStep(1);
            
            // Hide result
            const resultDiv = document.getElementById('calcResult');
            if (resultDiv) {
                resultDiv.style.display = 'none';
            }
        };
        
        // Calculator Functions
        window.calculateRenovation = function() {
            const basePrice = selectedSize * selectedType * 1000;
            const addonsTotal = selectedAddons.reduce((sum, addon) => sum + addon, 0);
            const totalPrice = basePrice + addonsTotal;
            
            // Update result display
            const resultDiv = document.getElementById('calcResult');
            const totalPriceEl = document.getElementById('totalPrice');
            const basePriceEl = document.getElementById('basePrice');
            const addonsPriceEl = document.getElementById('addonsPrice');
            
            // Update summary
            const summarySizeEl = document.getElementById('summarySize');
            const summaryTypeEl = document.getElementById('summaryType');
            const summaryAddonsEl = document.getElementById('summaryAddons');
            
            if (totalPriceEl && basePriceEl && addonsPriceEl) {
                totalPriceEl.textContent = `â‚ª${totalPrice.toLocaleString()}`;
                basePriceEl.textContent = `â‚ª${basePrice.toLocaleString()}`;
                addonsPriceEl.textContent = `â‚ª${addonsTotal.toLocaleString()}`;
                
                if (summarySizeEl && summaryTypeEl && summaryAddonsEl) {
                    summarySizeEl.textContent = selectedSizeText;
                    summaryTypeEl.textContent = selectedTypeText;
                    summaryAddonsEl.textContent = selectedAddons.length > 0 ? `${selectedAddons.length} ×ª×•×¡×¤×•×ª` : '×œ×œ× ×ª×•×¡×¤×•×ª';
                }
                
                if (resultDiv) {
                    resultDiv.style.display = 'block';
                    resultDiv.scrollIntoView({ behavior: 'smooth', block: 'center' });
                }
            }
        };
        
        // Calculator Step Navigation
        function initCalculator() {
            // Handle size selection
            wrapper.querySelectorAll('.shiputz24-calculator [data-step="1"] .shiputz24-calc-option').forEach(option => {
                option.addEventListener('click', function() {
                    // Remove previous selection
                    wrapper.querySelectorAll('.shiputz24-calculator [data-step="1"] .shiputz24-calc-option').forEach(opt => {
                        opt.classList.remove('selected');
                    });
                    // Add selection
                    this.classList.add('selected');
                    selectedSize = parseInt(this.dataset.value);
                    selectedSizeText = this.querySelector('.shiputz24-calc-option__text').textContent;
                    
                    // Move to next step
                    setTimeout(() => {
                        showStep(2);
                    }, 300);
                });
            });
            
            // Handle type selection
            wrapper.querySelectorAll('.shiputz24-calculator [data-step="2"] .shiputz24-calc-option').forEach(option => {
                option.addEventListener('click', function() {
                    // Remove previous selection
                    wrapper.querySelectorAll('.shiputz24-calculator [data-step="2"] .shiputz24-calc-option').forEach(opt => {
                        opt.classList.remove('selected');
                    });
                    // Add selection
                    this.classList.add('selected');
                    selectedType = parseFloat(this.dataset.value);
                    selectedTypeText = this.querySelector('.shiputz24-calc-option__text').textContent;
                    
                    // Move to next step
                    setTimeout(() => {
                        showStep(3);
                    }, 300);
                });
            });
            
            // Handle addon selection
            wrapper.querySelectorAll('.shiputz24-calculator [data-step="3"] .shiputz24-calc-checkbox').forEach(checkbox => {
                checkbox.addEventListener('click', function() {
                    const value = parseInt(this.dataset.value);
                    
                    if (this.classList.contains('selected')) {
                        // Remove selection
                        this.classList.remove('selected');
                        selectedAddons = selectedAddons.filter(addon => addon !== value);
                    } else {
                        // Add selection
                        this.classList.add('selected');
                        selectedAddons.push(value);
                    }
                });
            });
        }
        
        function showStep(stepNumber) {
            wrapper.querySelectorAll('.shiputz24-calculator .shiputz24-calc-step').forEach(step => {
                step.classList.remove('active');
            });
            
            // Update progress indicators
            wrapper.querySelectorAll('.shiputz24-calculator .shiputz24-calc-progress__dot').forEach((dot, index) => {
                dot.classList.remove('active', 'completed');
                if (index + 1 < stepNumber) {
                    dot.classList.add('completed');
                } else if (index + 1 === stepNumber) {
                    dot.classList.add('active');
                }
            });
            
            const targetStep = wrapper.querySelector(`.shiputz24-calculator [data-step="${stepNumber}"]`);
            if (targetStep) {
                setTimeout(() => {
                    targetStep.classList.add('active');
                    
                    // Restore previous selections when going back
                    if (stepNumber === 1 && selectedSize > 0) {
                        const selectedOption = targetStep.querySelector(`[data-value="${selectedSize}"]`);
                        if (selectedOption) selectedOption.classList.add('selected');
                    }
                    
                    if (stepNumber === 2 && selectedType > 0) {
                        const selectedOption = targetStep.querySelector(`[data-value="${selectedType}"]`);
                        if (selectedOption) selectedOption.classList.add('selected');
                    }
                    
                    if (stepNumber === 3 && selectedAddons.length > 0) {
                        selectedAddons.forEach(addonValue => {
                            const selectedAddon = targetStep.querySelector(`[data-value="${addonValue}"]`);
                            if (selectedAddon) selectedAddon.classList.add('selected');
                        });
                    }
                }, 100);
            }
        }
        
        // Initialize calculator when page loads
        setTimeout(() => {
            initCalculator();
        }, 500);
        
        // Smooth scrolling for anchor links
        wrapper.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });
        
        // Intersection Observer for animations
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };
        
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);
        
        // Observe sections for animation
        wrapper.querySelectorAll('.shiputz24-section').forEach(section => {
            section.style.opacity = '0';
            section.style.transform = 'translateY(30px)';
            section.style.transition = 'all 0.8s ease-out';
            observer.observe(section);
        });
    })();
    
    // Widget initialization
    document.addEventListener('DOMContentLoaded', function() {
        setTimeout(function() {
            if (typeof initAWGWidget === 'function') {
                initAWGWidget();
            }
            jQuery(document).trigger('awg:widget:loaded');
        }, 1000);
    });
    </script>

