<style>
    @import url('https://fonts.googleapis.com/css2?family=Assistant:wght@400;600;700&display=swap');
    
    #shiputz24-wrapper {
        --lp-primary: #1e5490;
        --lp-primary-dark: #2a5fa0;
        --lp-secondary: #ff3b3b;
        --lp-success: #25D366;
        --lp-warning: #FF9800;
        --lp-text-dark: #333333;
        --lp-text-light: #666666;
        --lp-text-muted: #777777;
        --lp-background: #ffffff;
        --lp-gray-light: #f8f8f8;
        --lp-gray-medium: #e0e0e0;
        --lp-shadow-soft: 0 3px 10px rgba(0,0,0,0.06);
        --lp-shadow-medium: 0 10px 30px rgba(0,0,0,0.12);
        --lp-shadow-hover: 0 10px 40px rgba(0,0,0,0.15);
        
        font-family: 'Assistant', 'Segoe UI', Tahoma, Arial, sans-serif;
        direction: rtl;
        text-align: right;
        color: var(--lp-text-dark);
        line-height: 1.6;
        margin: 0;
        padding: 0;
    }
    
    /* Hero Section */
    #shiputz24-wrapper .shiputz24-hero {
        position: relative;
        min-height: auto;
        background: linear-gradient(135deg, var(--lp-primary-dark) 0%, var(--lp-primary) 100%);
        display: flex;
        align-items: center;
        overflow: hidden;
        padding: 30px 0;
    }
    
    #shiputz24-wrapper .shiputz24-hero__animation {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        opacity: 0.1;
    }
    
    #shiputz24-wrapper .shiputz24-hero__house {
        position: absolute;
        right: 10%;
        top: 20%;
        width: 150px;
        height: 150px;
        opacity: 0.15;
    }
    
    #shiputz24-wrapper .shiputz24-hero__content {
        position: relative;
        max-width: 1200px;
        margin: 0 auto;
        padding: 40px 20px;
        z-index: 2;
    }
    
    #shiputz24-wrapper .shiputz24-hero__title {
        font-family: 'Assistant', sans-serif;
        font-size: 3em;
        font-weight: 700;
        color: white;
        margin-bottom: 15px;
        text-shadow: 2px 2px 4px rgba(0,0,0,0.3);
        animation: shiputz24-fadeInDown 1s ease-out;
        text-align: center;
    }
    
    #shiputz24-wrapper .shiputz24-hero__subtitle {
        font-size: 1.2em;
        color: rgba(255,255,255,0.9);
        margin-bottom: 30px;
        animation: shiputz24-fadeInUp 1s ease-out 0.3s both;
        text-align: center;
    }
    
    /* Hero Calculator */
    #shiputz24-wrapper .shiputz24-hero-calculator {
        background: rgba(255,255,255,0.95) !important;
        backdrop-filter: blur(10px) !important;
        padding: 30px !important;
        border-radius: 25px !important;
        box-shadow: 0 20px 60px rgba(0,0,0,0.3) !important;
        margin: 30px auto !important;
        max-width: 900px !important;
        border: 2px solid rgba(255,255,255,0.3) !important;
        animation: shiputz24-fadeIn 1s ease-out 0.6s both !important;
    }
    
    /* Hero Calculator Overrides */
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-options {
        display: grid !important;
        grid-template-columns: repeat(auto-fit, minmax(180px, 1fr)) !important;
        gap: 12px !important;
        margin-bottom: 25px !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-option {
        background: white !important;
        border: 2px solid var(--lp-gray-medium) !important;
        border-radius: 12px !important;
        padding: 15px !important;
        text-align: center !important;
        cursor: pointer !important;
        transition: all 0.3s ease !important;
        box-shadow: 0 3px 10px rgba(0,0,0,0.1) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-option:hover {
        border-color: var(--lp-primary) !important;
        transform: translateY(-2px) !important;
        box-shadow: 0 8px 25px rgba(30, 84, 144, 0.2) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-option.selected {
        background: linear-gradient(135deg, var(--lp-primary) 0%, var(--lp-primary-dark) 100%) !important;
        border-color: var(--lp-primary) !important;
        color: white !important;
        transform: translateY(-2px) !important;
        box-shadow: 0 8px 25px rgba(30, 84, 144, 0.4) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-checkboxes {
        display: grid !important;
        grid-template-columns: repeat(auto-fit, minmax(160px, 1fr)) !important;
        gap: 12px !important;
        margin-bottom: 25px !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-checkbox {
        background: white !important;
        border: 2px solid var(--lp-gray-medium) !important;
        border-radius: 12px !important;
        padding: 12px !important;
        text-align: center !important;
        cursor: pointer !important;
        transition: all 0.3s ease !important;
        box-shadow: 0 3px 10px rgba(0,0,0,0.1) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-checkbox:hover {
        border-color: var(--lp-success) !important;
        transform: translateY(-2px) !important;
        box-shadow: 0 8px 25px rgba(37, 211, 102, 0.2) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-checkbox.selected {
        background: linear-gradient(135deg, var(--lp-success) 0%, #128C7E 100%) !important;
        border-color: var(--lp-success) !important;
        color: white !important;
        transform: translateY(-2px) !important;
        box-shadow: 0 8px 25px rgba(37, 211, 102, 0.4) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-result {
        background: white !important;
        border: 3px solid var(--lp-primary) !important;
        border-radius: 20px !important;
        padding: 25px !important;
        margin-top: 25px !important;
        box-shadow: 0 10px 30px rgba(0,0,0,0.2) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-btn {
        background: linear-gradient(135deg, var(--lp-secondary) 0%, #ff6b6b 100%) !important;
        color: white !important;
        border: none !important;
        padding: 14px 35px !important;
        border-radius: 50px !important;
        font-size: 1.1em !important;
        font-weight: 700 !important;
        cursor: pointer !important;
        transition: all 0.3s ease !important;
        display: block !important;
        margin: 20px auto !important;
        box-shadow: 0 8px 25px rgba(255, 59, 59, 0.4) !important;
        animation: shiputz24-pulse 2s infinite !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-progress {
        display: flex !important;
        justify-content: center !important;
        gap: 12px !important;
        margin-bottom: 20px !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-progress__dot {
        width: 14px !important;
        height: 14px !important;
        border-radius: 50% !important;
        background: var(--lp-gray-medium) !important;
        transition: all 0.3s ease !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-progress__dot.active {
        background: var(--lp-primary) !important;
        transform: scale(1.3) !important;
        box-shadow: 0 0 0 3px rgba(30, 84, 144, 0.3) !important;
    }
    
    #shiputz24-wrapper .shiputz24-hero-calculator .shiputz24-calc-progress__dot.completed {
        background: var(--lp-success) !important;
        box-shadow: 0 0 0 2px rgba(37, 211, 102, 0.3) !important;
    }
    
    /* Container */
    #shiputz24-wrapper .shiputz24-container {
        max-width: 1200px;
        margin: 0 auto;
        padding: 0 20px;
    }
    
    /* Sections */
    #shiputz24-wrapper .shiputz24-section {
        padding: 60px 0;
        position: relative;
    }
    
    #shiputz24-wrapper .shiputz24-section:nth-child(even) {
        background: var(--lp-gray-light);
    }
    
    #shiputz24-wrapper .shiputz24-section__title {
        font-family: 'Assistant', sans-serif;
        font-size: 2.5em;
        font-weight: 600;
        color: var(--lp-primary);
        margin-bottom: 30px;
        position: relative;
        padding-right: 25px;
    }
    
    #shiputz24-wrapper .shiputz24-section__title::before {
        content: '';
        position: absolute;
        right: 0;
        top: 0;
        bottom: 0;
        width: 4px;
        background: linear-gradient(to bottom, var(--lp-secondary), var(--lp-warning));
        border-radius: 2px;
    }
    
    /* Lead paragraph */
    #shiputz24-wrapper .shiputz24-lead {
        font-size: 1.3em;
        line-height: 1.8;
        color: var(--lp-text-light);
        margin-bottom: 40px;
    }
    
    /* Widget Container */
    #shiputz24-wrapper .shiputz24-widget-container {
          width: 100vw;
        max-width: 100vw;
        position: relative;
        left: 50%;
        right: 50%;
        margin-left: -50vw;
        margin-right: -50vw;
        box-sizing: border-box;
    }
    
    /* Button Primary */
    #shiputz24-wrapper .shiputz24-btn {
        display: inline-flex;
        align-items: center;
        gap: 12px;
        padding: 16px 32px;
        border-radius: 50px;
        font-family: 'Assistant', sans-serif;
        font-weight: 600;
        font-size: 1.1em;
        text-decoration: none;
        transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
        position: relative;
        overflow: hidden;
        border: none;
        cursor: pointer;
    }
    
    #shiputz24-wrapper .shiputz24-btn--primary {
        background: linear-gradient(135deg, var(--lp-primary) 0%, var(--lp-primary-dark) 100%);
        color: white;
        box-shadow: 0 5px 20px rgba(30, 84, 144, 0.3);
    }
    
    #shiputz24-wrapper .shiputz24-btn--primary:hover {
        transform: translateY(-3px);
        box-shadow: 0 8px 30px rgba(30, 84, 144, 0.4);
    }
    
    /* Benefits Grid */
    #shiputz24-wrapper .shiputz24-benefits {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 30px;
        margin: 50px 0;
    }
    
    #shiputz24-wrapper .shiputz24-benefit-card {
        background: white;
        padding: 30px;
        border-radius: 20px;
        box-shadow: var(--lp-shadow-soft);
        border-top: 4px solid var(--lp-primary);
        position: relative;
        overflow: hidden;
    }
    
    #shiputz24-wrapper .shiputz24-benefit-card__icon {
        width: 60px;
        height: 60px;
        background: linear-gradient(135deg, var(--lp-primary) 0%, var(--lp-secondary) 100%);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 20px;
    }
    
    #shiputz24-wrapper .shiputz24-benefit-card__title {
        font-size: 1.4em;
        font-weight: 600;
        color: var(--lp-primary);
        margin-bottom: 15px;
    }
    
    /* Feature List */
    #shiputz24-wrapper .shiputz24-feature-list {
        list-style: none;
        padding: 0;
        margin: 30px 0;
    }
    
    #shiputz24-wrapper .shiputz24-feature-item {
        display: flex;
        align-items: flex-start;
        gap: 15px;
        padding: 15px 0;
        border-bottom: 1px solid rgba(0,0,0,0.05);
        transition: all 0.3s ease;
    }
    
    #shiputz24-wrapper .shiputz24-feature-item:hover {
        background: var(--lp-gray-light);
        padding-right: 15px;
        border-radius: 10px;
    }
    
    #shiputz24-wrapper .shiputz24-feature-icon {
        flex-shrink: 0;
        width: 24px;
        height: 24px;
        background: var(--lp-success);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        color: white;
        font-weight: bold;
        font-size: 0.9em;
    }
    
    /* WhatsApp Section */
    #shiputz24-wrapper .shiputz24-whatsapp-section {
        margin: 60px 0;
        padding: 40px;
        background: linear-gradient(135deg, var(--lp-success) 0%, #128C7E 100%);
        border-radius: 20px;
        text-align: center;
        position: relative;
        overflow: hidden;
    }
    
    #shiputz24-wrapper .shiputz24-whatsapp-section::before {
        content: '';
        position: absolute;
        top: -50%;
        left: -50%;
        width: 200%;
        height: 200%;
        background: radial-gradient(circle, rgba(255,255,255,0.1) 0%, transparent 70%);
        animation: shiputz24-rotate 20s linear infinite;
    }
    
    #shiputz24-wrapper .shiputz24-whatsapp-cta__title {
        color: white;
        font-size: 2em;
        font-weight: 700;
        margin-bottom: 10px;
        position: relative;
        z-index: 2;
    }
    
    #shiputz24-wrapper .shiputz24-whatsapp-cta__text {
        color: rgba(255,255,255,0.9);
        font-size: 1.2em;
        margin-bottom: 25px;
        position: relative;
        z-index: 2;
    }
    
    #shiputz24-wrapper .shiputz24-whatsapp-cta__btn {
        display: inline-flex;
        align-items: center;
        gap: 12px;
        background: white;
        color: var(--lp-success);
        padding: 16px 32px;
        border-radius: 50px;
        text-decoration: none;
        font-weight: 700;
        font-size: 1.1em;
        transition: all 0.3s ease;
        box-shadow: 0 5px 20px rgba(0,0,0,0.2);
        position: relative;
        z-index: 2;
    }
    
    #shiputz24-wrapper .shiputz24-whatsapp-cta__btn:hover {
        transform: translateY(-3px) scale(1.05);
        box-shadow: 0 8px 30px rgba(0,0,0,0.3);
    }
    
    /* Process Steps */
    #shiputz24-wrapper .shiputz24-process {
        margin: 60px 0;
    }
    
    #shiputz24-wrapper .shiputz24-process__grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 30px;
        margin-top: 40px;
    }
    
    #shiputz24-wrapper .shiputz24-process-step {
        text-align: center;
        padding: 30px 20px;
        position: relative;
    }
    
    #shiputz24-wrapper .shiputz24-process-step__number {
        width: 80px;
        height: 80px;
        background: linear-gradient(135deg, var(--lp-primary) 0%, var(--lp-secondary) 100%);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 2em;
        font-weight: bold;
        color: white;
        margin: 0 auto 20px;
        box-shadow: 0 10px 30px rgba(30, 84, 144, 0.3);
    }
    
    #shiputz24-wrapper .shiputz24-process-step__title {
        font-size: 1.3em;
        font-weight: 600;
        color: var(--lp-primary);
        margin-bottom: 15px;
    }
    
    /* Calculator */
    #shiputz24-wrapper .shiputz24-calculator {
        background: white;
        padding: 40px;
        border-radius: 20px;
        box-shadow: var(--lp-shadow-medium);
        margin: 40px 0;
        border: 2px solid var(--lp-primary);
    }
    
    #shiputz24-wrapper .shiputz24-calculator__title {
        font-size: 1.8em;
        font-weight: 600;
        color: var(--lp-primary);
        margin-bottom: 30px;
        text-align: center;
    }
    
    /* Calculator Navigation */
    #shiputz24-wrapper .shiputz24-calc-nav {
        margin-bottom: 15px !important;
        text-align: left !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-back-btn {
        background: var(--lp-gray-medium) !important;
        color: var(--lp-text-dark) !important;
        border: none !important;
        padding: 8px 16px !important;
        border-radius: 25px !important;
        font-size: 0.9em !important;
        font-weight: 600 !important;
        cursor: pointer !important;
        transition: all 0.3s ease !important;
        display: inline-flex !important;
        align-items: center !important;
        gap: 5px !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-back-btn:hover {
        background: var(--lp-primary) !important;
        color: white !important;
        transform: translateX(-3px) !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-edit-btn {
        background: var(--lp-warning) !important;
        color: white !important;
        border: none !important;
        padding: 10px 20px !important;
        border-radius: 25px !important;
        font-size: 0.9em !important;
        font-weight: 600 !important;
        cursor: pointer !important;
        transition: all 0.3s ease !important;
        display: inline-flex !important;
        align-items: center !important;
        gap: 8px !important;
        margin-bottom: 15px !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-edit-btn:hover {
        background: #e68900 !important;
        transform: translateY(-2px) !important;
        box-shadow: 0 5px 15px rgba(255, 152, 0, 0.3) !important;
    }
    
    /* Calculator Summary */
    #shiputz24-wrapper .shiputz24-calc-result__summary {
        background: rgba(30, 84, 144, 0.05) !important;
        border-radius: 15px !important;
        padding: 20px !important;
        margin: 20px 0 !important;
        border: 1px solid rgba(30, 84, 144, 0.1) !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-summary__item {
        display: flex !important;
        justify-content: space-between !important;
        align-items: center !important;
        padding: 8px 0 !important;
        border-bottom: 1px solid rgba(30, 84, 144, 0.1) !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-summary__item:last-child {
        border-bottom: none !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-summary__label {
        color: var(--lp-text-light) !important;
        font-weight: 600 !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-restart {
        text-align: center !important;
        margin-top: 15px !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-restart-btn {
        background: transparent !important;
        color: var(--lp-text-light) !important;
        border: 1px solid var(--lp-gray-medium) !important;
        padding: 8px 16px !important;
        border-radius: 20px !important;
        font-size: 0.9em !important;
        cursor: pointer !important;
        transition: all 0.3s ease !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-restart-btn:hover {
        border-color: var(--lp-secondary) !important;
        color: var(--lp-secondary) !important;
        background: rgba(255, 59, 59, 0.05) !important;
    }
    #shiputz24-wrapper .shiputz24-calc-steps {
        max-width: 100%;
    }
    
    #shiputz24-wrapper .shiputz24-calc-step {
        display: none;
        opacity: 0;
        transform: translateX(20px);
        transition: all 0.5s ease;
    }
    
    #shiputz24-wrapper .shiputz24-calc-step.active {
        display: block !important;
        opacity: 1 !important;
        transform: translateX(0) !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-step__title {
        font-size: 1.4em;
        font-weight: 600;
        color: var(--lp-primary);
        margin-bottom: 25px;
        text-align: center;
    }
    
    /* Calculator Options */
    #shiputz24-wrapper .shiputz24-calc-options {
        display: grid !important;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)) !important;
        gap: 15px !important;
        margin-bottom: 30px !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-option {
        background: var(--lp-gray-light) !important;
        border: 2px solid var(--lp-gray-medium) !important;
