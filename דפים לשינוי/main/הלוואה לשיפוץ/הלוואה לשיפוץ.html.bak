        transition: all 0.3s ease !important;
        position: relative !important;
        overflow: hidden !important;
        display: block !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-option:hover {
        border-color: var(--lp-primary) !important;
        transform: translateY(-3px) !important;
        box-shadow: var(--lp-shadow-hover) !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-option.selected {
        background: linear-gradient(135deg, var(--lp-primary) 0%, var(--lp-primary-dark) 100%) !important;
        border-color: var(--lp-primary) !important;
        color: white !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-option.selected .shiputz24-calc-option__price,
    #shiputz24-wrapper .shiputz24-calc-option.selected .shiputz24-calc-option__subtitle {
        color: rgba(255,255,255,0.9) !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-option__icon {
        font-size: 2.5em !important;
        margin-bottom: 10px !important;
        display: block !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-option__text {
        font-size: 1.2em !important;
        font-weight: 600 !important;
