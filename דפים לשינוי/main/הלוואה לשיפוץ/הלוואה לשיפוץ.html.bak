    #shiputz24-wrapper .shiputz24-calc-checkbox.selected {
        background: linear-gradient(135deg, var(--lp-success) 0%, #128C7E 100%) !important;
        border-color: var(--lp-success) !important;
        color: white !important;
    }
    
    #shiputz24-wrapper .shiputz24-calc-checkbox.selected .shiputz24-calc-checkbox__price {
        color: rgba(255,255,255,0.9) !important;
    }
    
